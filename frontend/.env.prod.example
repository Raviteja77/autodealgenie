# Production Environment Variables for GCP Free Tier Frontend
# Copy this to .env.prod.local and update values

# API Configuration - Update with your Cloud Run backend URL
NEXT_PUBLIC_API_URL=https://autodealgenie-backend-prod-xxxx.a.run.app
NEXT_PUBLIC_API_VERSION=v1

# Mock Services - MUST be false in production
NEXT_PUBLIC_USE_MOCK=false

# Node Environment
NODE_ENV=production
